{
  "name": "talentguard-buyer-intelligence",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "workers": "node scripts/start-workers.js",
    "workers:dev": "concurrently \"npm run dev\" \"npm run workers\"",
    "redis:start": "docker run -d --name talentguard-redis -p 6379:6379 redis:7-alpine",
    "redis:stop": "docker stop talentguard-redis && docker rm talentguard-redis",
    "queue:status": "curl http://localhost:3000/api/jobs",
    "queue:clean": "curl -X DELETE http://localhost:3000/api/jobs?action=clean",
    "setup:security": "node scripts/install-security-deps.js",
    "setup:auth": "node scripts/setup-auth.js",
    "docker:dev": "docker-compose up --build",
    "docker:dev:detached": "docker-compose up --build -d",
    "docker:dev:logs": "docker-compose logs -f",
    "docker:dev:logs:app": "docker-compose logs -f app",
    "docker:dev:logs:workers": "docker-compose logs -f workers",
    "docker:stop": "docker-compose down",
    "docker:stop:volumes": "docker-compose down -v",
    "docker:restart": "docker-compose restart",
    "docker:restart:app": "docker-compose restart app",
    "docker:rebuild": "docker-compose down && docker-compose up --build",
    "docker:shell": "docker-compose exec app sh",
    "docker:shell:workers": "docker-compose exec workers sh",
    "docker:tools": "docker-compose --profile tools up -d",
    "docker:tools:stop": "docker-compose --profile tools down",
    "docker:db:shell": "docker-compose exec postgres psql -U postgres -d talentguard",
    "docker:redis:cli": "docker-compose exec redis redis-cli",
    "docker:health": "docker-compose ps",
    "docker:clean": "docker system prune -f && docker volume prune -f",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:unit": "cross-env NODE_ENV=test jest --testPathPattern=__tests__/unit",
    "test:integration": "cross-env NODE_ENV=test jest --testPathPattern=__tests__/integration",
    "test:api": "cross-env NODE_ENV=test jest --testPathPattern=__tests__/api",
    "test:components": "cross-env NODE_ENV=test jest --testPathPattern=__tests__/components",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:api && npm run test:components && npm run test:e2e",
    "test:docker": "docker-compose exec app npm run test",
    "test:docker:watch": "docker-compose exec app npm run test:watch",
    "test:docker:coverage": "docker-compose exec app npm run test:coverage",
    "analyze": "cross-env ANALYZE=true npm run build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser npm run build"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "jose": "^5.2.2",
    "next-auth": "^4.24.5",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/supabase-js": "^2.53.0",
    "@tabler/icons-react": "^3.34.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "airtable": "^0.12.2",
    "autoprefixer": "^10.4.16",
    "bullmq": "^5.56.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.15",
    "ioredis": "^5.7.0",
    "jsforce": "^3.9.1",
    "lucide-react": "^0.525.0",
    "next": "^14.2.15",
    "postcss": "^8.4.47",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.48.2",
    "sonner": "^1.4.0",
    "swr": "^2.3.4",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "concurrently": "^9.2.0",
    "tsx": "^4.20.3",
    "@jest/types": "^29.6.3",
    "@playwright/test": "^1.44.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "msw": "^2.2.14",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "ts-jest": "^29.1.2",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
